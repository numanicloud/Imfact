{
    "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/master/src/NerdBank.GitVersioning/version.schema.json",
    "version": "1.0.0-alpha.{height}",
    "nuGetPackageVersion": {
      "semVer": 2.0
    },
    "publicReleaseRefSpec": [
      "^refs/heads/main$",
      "^refs/heads/v\\d+(?:\\.\\d+)?(?:\\.\\d+)?$"
    ],
    "cloudBuild": {
      "buildNumber": {
        "enabled": true
      }
    },
    "release": {
      "branchName": "nerdbank/v{version}"
    }
  }